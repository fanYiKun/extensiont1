{
  "name": "给视频加些图钉",
  "description": "手动给视频分段的v1.0.0",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    // "<all_url>",
    "activeTab",
    "scripting",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "JS/jquery-3.6.min.js",
        "bootstrap-5.1.3-dist/js/bootstrap.bundle.min.js"
      ],
      "css": [
        "bootstrap-5.1.3-dist/css/bootstrap.min.css"
      ],
      "run_at": "document_start"
    }
  ]
}